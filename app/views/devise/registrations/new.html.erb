<h2>Registro</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

	<p><%= f.label :first_name, 'Nombre' %><br />
	  <%= f.text_field :first_name %></p>
	
	<p><%= f.label :last_name, 'Apellido' %><br />
	  <%= f.text_field :last_name %></p>
	
	<p><%= f.label :phone, 'Telefono' %><br />
	  <%= f.text_field :phone %></p>

  <p><%= f.label :email, 'Correo electronico' %><br />
  <%= f.text_field :email %></p>

  <p><%= f.label :password %><br />
  <%= f.password_field :password %></p>

  <p><%= f.label :password_confirmation, 'Confirmar password' %><br />
  <%= f.password_field :password_confirmation %></p>

  <p><%= f.submit "Registrarme" %></p>
<% end %>

<%= render :partial => "devise/shared/links" %>
